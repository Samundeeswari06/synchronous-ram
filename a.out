#! /usr/local/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x28038060 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x280381f0 .scope module, "tb_sync_ram" "tb_sync_ram" 3 3;
 .timescale 0 0;
v0x2804dc40_0 .var "addr", 2 0;
v0x2804dd20_0 .var "clk", 0 0;
v0x2804ddf0_0 .var "din", 7 0;
v0x2804def0_0 .net "dout", 7 0, v0x2804d8f0_0;  1 drivers
v0x2804dfc0_0 .var "we", 0 0;
S_0x28012580 .scope module, "uut" "sync_ram" 3 10, 4 2 0, S_0x280381f0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "we";
    .port_info 2 /INPUT 3 "addr";
    .port_info 3 /INPUT 8 "din";
    .port_info 4 /OUTPUT 8 "dout";
v0x280127a0_0 .net "addr", 2 0, v0x2804dc40_0;  1 drivers
v0x2804d740_0 .net "clk", 0 0, v0x2804dd20_0;  1 drivers
v0x2804d800_0 .net "din", 7 0, v0x2804ddf0_0;  1 drivers
v0x2804d8f0_0 .var "dout", 7 0;
v0x2804d9d0 .array "mem", 7 0, 7 0;
v0x2804dae0_0 .net "we", 0 0, v0x2804dfc0_0;  1 drivers
E_0x28012760 .event posedge, v0x2804d740_0;
    .scope S_0x28012580;
T_0 ;
    %wait E_0x28012760;
    %load/vec4 v0x2804dae0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x2804d800_0;
    %load/vec4 v0x280127a0_0;
    %pad/u 5;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x2804d9d0, 0, 4;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x280127a0_0;
    %pad/u 5;
    %ix/vec4 4;
    %load/vec4a v0x2804d9d0, 4;
    %assign/vec4 v0x2804d8f0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x280381f0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x2804dd20_0;
    %inv;
    %store/vec4 v0x2804dd20_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x280381f0;
T_2 ;
    %vpi_call/w 3 19 "$monitor", "Time = %0t | WE = %b | Addr = %d | DIN = %h | DOUT = %h", $time, v0x2804dfc0_0, v0x2804dc40_0, v0x2804ddf0_0, v0x2804def0_0 {0 0 0};
    %vpi_call/w 3 20 "$dumpfile", "sync_ram.vcd" {0 0 0};
    %vpi_call/w 3 21 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x2804dd20_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x2804dfc0_0, 0, 1;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x2804dc40_0, 0, 3;
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x2804ddf0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x2804dc40_0, 0, 3;
    %pushi/vec4 85, 0, 8;
    %store/vec4 v0x2804ddf0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x2804dc40_0, 0, 3;
    %pushi/vec4 240, 0, 8;
    %store/vec4 v0x2804ddf0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x2804dfc0_0, 0, 1;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x2804dc40_0, 0, 3;
    %delay 10, 0;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x2804dc40_0, 0, 3;
    %delay 10, 0;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x2804dc40_0, 0, 3;
    %delay 10, 0;
    %vpi_call/w 3 31 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "testbench.sv";
    "design.sv";
